{
    "name": "clarkwinkelmann/flarum-ext-carving-contest",
    "description": "Pumpkin carving contest",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-carving-contest",
        "forum": "https://discuss.flarum.org/d/21828"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\CarvingContest\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Carving Contest",
            "category": "feature",
            "icon": {
                "name": "fas fa-spider",
                "backgroundColor": "#db5800",
                "color": "#030303"
            }
